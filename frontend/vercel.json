{
  "rewrites": [
    { 
      "source": "/api/:path*", 
      "destination": "http://ec2-18-233-151-230.compute-1.amazonaws.com/api/:path*" 
    },
    { 
      "source": "/(.*)", 
      "destination": "/index.html" 
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "framework": "create-react-app",
  "cleanUrls": true
}
